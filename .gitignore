# Update .gitignore to prevent future issues
cat > .gitignore << 'EOF'
# Build artifacts
.buildozer/
bin/
*.apk
*.aab
dist/

# Python cache
__pycache__/
*.pyc
*.pyo
*.pyd

# Environment
.env
venv/
.venv/

# OS
.DS_Store
Thumbs.db

# Logs
*.log
build.log

# Local configurations
local_config.json
EOF
